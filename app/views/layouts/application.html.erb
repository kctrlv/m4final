<!DOCTYPE html>
<html>
<head>
  <title>URLockBox</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<% flash.each do |key, val| %>
  <div class="alert alert-<%= key %>"><%= val %></div>
<% end %>
<%= link_to 'Sign Out', logout_path if current_user %>
<div class="container">
  <%= yield %>
</div>

</body>
</html>
